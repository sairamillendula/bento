<% if @shipping_rates %>
  $('#shipping-rate-feedback').empty().append('<%= j render("results") %>').show();
  <% if @shipping_estimate %>
    $('#shipping-price').text("<%= @shipping_estimate.is_free? ? 'FREE' : number_to_currency(@shipping_estimate.price) %>");
    $('#shipping').show();
  <% else %>
    $('#shipping').hide();
  <% end %>
<% else %>
  $('#shipping-rate-feedback').empty().append("Error: country can't be blank").show();
  $('#shipping').hide();
<% end %>


