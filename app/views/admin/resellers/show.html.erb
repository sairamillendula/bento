<% reseller = @user.reseller_request %>
<%= page_title(reseller.business_name) do %>
  <%= status_tag(reseller.approved? ? "Approved" : "Declined", level: (reseller.approved? ? "success" : "warning") ) %>
<% end %>

<p><b>Submitted: </b><%= l reseller.created_at, format: :long %></p>

<p><b>User: </b><%= @user.full_name %> (<%= @user.email %>)</p>

<p><b>Location: </b><%= reseller.city %> - <%= reseller.country %></p>

<%= simple_format(reseller.who_are_you) %>