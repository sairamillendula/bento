<%= page_title("#{t 'shipping_rates.new'}") %>

<%= simple_form_for([:admin, @shipping_country], html: {class: 'form-horizontal'}) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :country, collection: Country.all %>
  </div>
  
  <div class="form-actions">
    <%= submit_or_cancel(f) %>
  </div>
<% end %>